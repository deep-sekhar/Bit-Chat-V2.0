extends base.pug 

block scripts 
    script
        include ../static/index.js

block style 
    style
        include ../static/style.css
    style
        include ../static/contactstyle.css

block content
    h1(style="margin:10px") CONTACT US
    div
        form#contact(action='' method='POST')
          .enter
            input#name.enter1(type='text' name='name' placeholder='name')
          .enter
            input#age.enter1(type='tel' name='age' placeholder='age')
          .enter
            textarea#address.enter1(name='address' cols='30' rows='10' placeholder='address')
          button.btn(type='submit') SUBMIT